                                </asn1Boolean>
                                <asn1Integer identifier="pathLenConstraint">
                                    <value>2</value>
                                </asn1Integer>
                            </asn1Sequence>
                        </asn1EncapsulatingOctetString>
                    </asn1Sequence>
                </asn1Sequence>
